<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head>

@@INCLUDE@@ @_SEP_@@_TEMPLATE_ROOT_@@_SEP_@@_LANG_@@_SEP_@header.thtml "@_PAGE-TITLE_@"

@@INCLUDE@@ @_SEP_@@_TEMPLATE_ROOT_@@_SEP_@header_includes.thtml 

</head>

<body>
<div id="wrapper">
   
@@INCLUDE@@ @_SEP_@@_TEMPLATE_ROOT_@@_SEP_@@_LANG_@@_SEP_@main_menu.thtml 

   <div id="page">
       <div id="page-bgtop">
           <div id="page-bgbtm">
               <div id="content">
@_MODEL-MENU_@
      
               <form action="" method="post" id="mainform" name="mainform">
                  <input type='hidden' name='random_string' value="@_RANDOM_STRING_@" />
                  
                  
                  <div id="modelContentInput">
                     <table>
                     <tr>
                     <td>
                     @_GALLERY-SECTION_@
                     </td>
                      <td align='right'>
                     @_CONTROL-SECTION_@
                     </td>
                     </tr>
                     <tr>
                     <td colspan='2' align='center'>
                     @_CONTENT-SECTION_@
                     </td>
                     </tr>
                     </table>
                  </div> <!-- model content -->      
                  <div style="clear: both;">&nbsp;</div>
                  <input type='hidden' name='which_page' id='which_page' value="@_WHICH_PAGE_@" /> 
                  <input type='hidden' name='which_section' id='which_section' value="output_page" /> 
      
              </form>
        </div> <!-- content -->
      </div> <!-- page-bgbtm -->
     </div> <!-- page-bgtop -->
   </div> <!-- page -->
</div> <!-- wrapper -->

@@INCLUDE@@ @_SEP_@@_TEMPLATE_ROOT_@@_SEP_@@_LANG_@@_SEP_@footer.thtml

</body>
</html>
