html { overflow: scroll; }
body {
  margin: 0;
  padding: 0;
  background: #ffffff;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 12px;
  color: #464032;
}
h1, h2, h3 {
  margin: 0;
  padding: 0;
  font-weight: normal;
  color: #221d1d;
}
h1 { font-size: 2em; }
h2 {
  font-size: 2.4em;
  margin-bottom: 10px;
}
h3 { font-size: 1.6em; }
p, ul, ol {
