.ve_clone_table { border: 1px dotted silver; }
.ve_clone_table td {
  padding-left: 1em;
  padding-right: 0.5em;
  border-right: 1px dotted silver;
  border-bottom: 1px dotted silver;
  text-align: right;
}
.ve_clone_table th {
  padding-left: 1em;
  padding-right: 0.5em;
  border-right: 1px dotted silver;
  border-bottom: 1px dotted silver;
  text-align: right;
}
.popup_container_table td { vertical-align: top; }
td.result_set { border-right: 2px dashed silver; }
table.control_section { border: 1px dotted silver; }
.pre_change { color: #b53a30; }
.post_change { color: #165b9b; }
.inputtable {
  border-collapse: collapse;
  text-align: left;
  padding: 8pt;
}
.inputtable h1 {
  color: #3366cc;
  font-size: 14pt;
  font-weight: normal;
}
.inputtable h3 { font-size: 13pt; }
.inputtable h4 { font-size: 12pt; }
.lightform {
	width: 449px;
	float: left;
	height: 160px;
	border: 1px solid #AAAAAA;
	margin: 0 5px 5px 0;
	padding: 5px;
	-moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -khtml-border-radius: 4px;
    border-radius: 4px;
	position: relative;
	background-color: #F9F9F9;
}
.designreformblock {
	float: left;
	border: 1px solid #AAAAAA;
	margin: 0 5px 5px 0;
	padding: 5px;
	-moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -khtml-border-radius: 4px;
    border-radius: 4px;
	position: relative;
	background-color: #F9F9F9;
}
.treenavigation {
	background-color: #EEEEEE;
	min-height: 500px;	
}
.lightbox {
	clear: both;
	width: 906px;
	height: 200px;
	border: 1px solid #AAAAAA;
	margin: 0 5px 5px 0;
	padding: 5px;
	-moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    -khtml-border-radius: 4px;
    border-radius: 4px;
	position: relative;
}
.lightform h3, .designreformblock h3 {
	margin-bottom: 20px;
	border-bottom: dotted 1px #999999;	
}
.formbuttons {
	position: absolute;
	bottom: 0;
	right: 0;
	text-align: right;
	background-color: #EEE;
	width: 100%;
}
#page .ui-tabs .ui-tabs-panel {
    padding: 1em 0 0 0;
}

.rowElem td {

}
.rowElem th {
	color: #666;
	font-size: 1em;
}
.users {
	position: absolute;
	top: 24px;
	right: 10px;
	z-index: 9999;	
	font-size:80%;
	display: none;
}

.results_block{
        display: block;
        width: 100%;
        padding: 4px;
}

.headerRow th{
	text-align: right;
}
